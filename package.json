{
  "name": "ensembl-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for Ensembl genomics database",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "start:prod": "node dist/src/index.js",
    "start:bun": "bun src/index.ts",
    "dev": "tsx --watch src/index.ts",
    "dev:bun": "bun --watch src/index.ts",
    "test": "tsx tests/run-all-tests.js",
    "test:normalizer": "tsx tests/input-normalizer.test.ts",
    "test:meta": "tsx tests/test-meta.js",
    "test:lookup": "tsx tests/test-lookup.js",
    "test:sequence": "tsx tests/test-sequence.js",
    "test:overlap": "tsx tests/test-feature-overlap.js",
    "test:regulatory": "tsx tests/test-regulatory.js",
    "test:protein": "tsx tests/test-protein-features.js",
    "test:mapping": "tsx tests/test-mapping.js",
    "test:compara": "tsx tests/test-compara.js",
    "test:variation": "tsx tests/test-variation.js",
    "test:ontotax": "tsx tests/test-ontotax.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "docker": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "mcp",
    "ensembl",
    "genomics",
    "bioinformatics"
  ],
  "author": "",
  "license": "MIT"
}
